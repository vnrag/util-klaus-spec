{
  "name": "klaus-spec",
  "private": true,
  "version": "1.0.0",
  "description": "This repository describes the specification of a Klaus module and includes a parser for JSON data.",
  "main": "dist/klaus-spec.js",
  "types": "./dist/types/klaus-spec.d.ts",
  "typings": "./dist/types/klaus-spec.d.ts",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "webpack",
    "build": "npm run build:types && npm run build:js",
    "watch": "npm run build:js -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://git.vnr-software-solutions.de:web/klaus-spec.git"
  },
  "author": "",
  "license": "ISC",
  "browserslist": [
    "last 2 version",
    "> 5%",
    "IE > 9"
  ],
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/he": "^1.1.0",
    "@types/mustache": "^0.8.32",
    "@types/semver": "^6.0.1",
    "babel-loader": "^8.0.6",
    "typescript": "^3.5.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5"
  },
  "dependencies": {
    "he": "^1.2.0",
    "mustache": "^3.0.1",
    "semver": "^6.1.2"
  }
}
